<p id="notice"><%= notice %></p>

<h1>Listing Money</h1>

<table>
  <thead>
    <tr>
      <th>Kbn</th>
      <th>Price</th>
      <th>Date</th>
      <th>Family</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @money.each do |money| %>
      <tr>
        <td><%= money.kbn %></td>
        <td><%= money.price %></td>
        <td><%= money.date %></td>
        <td><%= money.family_id %></td>
        <td><%= money.category_id %></td>
        <td><%= link_to 'Show', money %></td>
        <td><%= link_to 'Edit', edit_money_path(money) %></td>
        <td><%= link_to 'Destroy', money, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Money', new_money_path %>
